{
  "name": "phpbb2flarum",
  "author": "v.florio",
  "version": "1.0.0",
  "license": "MIT",
  "description": "phpBB to Flarum mySQL migration tool",
  "scripts": {
    "dev": "nodemon",
    "start": "node ./bin/convert.js",
    "build": "npm-run-all clean transpile",
    "transpile": "babel ./src --out-dir bin",
    "clean": "rimraf bin"
  },
  "dependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "mysql": "github:mysqljs/mysql",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "nodemonConfig": {
    "exec": "npm run build",
    "watch": [
      "src/*"
    ]
  }
}
